<script>
  var pageLoadTime = (new Date()).getTime();
  var reloading = false;

  // Refresh the page every 30 seconds. This cant just be done using a
  // refresh in the header tag because it doesnt work after iPhone
  // turned off and then started again. 
  // Note: This causes iPhones at least to not auto-turn off. Instead,
  // users need to manually turn off their device. But it is nice not having
  // to hit the refresh button all the time. 
  // Note: The reason calling reloadIfNeeded() every second is so that
  // if phone turned on and user goes to the page it will realize right
  // away that it should reload instead of waiting 30 seconds.
  // Note: reason need to check to see if already reloading is because
  // if connection is lost reloadIfNeeded() is still called every 300msec.
  // If would call reload() every 300msec then browser gets confused and it
  // can hang an iPhone completely!
  function reloadIfNeeded() {
    var currentTime = (new Date()).getTime();
    if (currentTime > pageLoadTime + 30*1000 && reloading==false) {
      reloading = true;
      location.reload();
    }
  }

  setInterval('reloadIfNeeded()', 300);
</script>

