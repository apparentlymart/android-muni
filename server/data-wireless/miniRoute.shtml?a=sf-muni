<script>
// This function is called when user enters a stop id.
// It jumps to the prediction page for the stop specified.
function stopIdEntered() {
  var stopId = document.theForm.stopId.value;
  var href = "/wireless/miniPrediction.shtml?a=sf-muni&stopId=" + stopId;
  window.location.href = href;
}
</script>
 
